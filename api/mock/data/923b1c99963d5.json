        {
          "self": "https://jira.xxx.com/rest/api/2/issue/611975/comment/1195227",
          "id": "1195227",
          "author": {
            "self": "https://jira.xxx.com/rest/api/2/user?username=leiychen",
            "name": "leiychen",
            "key": "leiychen",
